Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"topo1", "[", "n_", "]"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "^", "n"}], " ", "imI", " ", 
     RowBox[{"Pi", "^", 
      RowBox[{"(", 
       RowBox[{"2", "-", "eps"}], ")"}]}], " ", 
     RowBox[{
      RowBox[{"Gamma", "[", 
       RowBox[{"n", "+", "eps", "-", "2"}], "]"}], "/", 
      RowBox[{"Gamma", "[", "n", "]"}]}], " ", 
     RowBox[{"1", "/", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"mUV", "^", "2"}], ")"}], "^", 
       RowBox[{"(", 
        RowBox[{"n", "+", "eps", "-", "2"}], ")"}]}]}], "*", 
     RowBox[{"1", "/", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"2", " ", "Pi"}], ")"}], "^", 
       RowBox[{"(", 
        RowBox[{"4", "-", 
         RowBox[{"2", "*", "eps"}]}], ")"}]}]}]}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", "  ", 
   RowBox[{"We", " ", "take", " ", "the", " ", "LTD", " ", "measure", " ", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox["1", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"2", " ", "Pi"}], ")"}], 
         RowBox[{"4", "-", 
          RowBox[{"2", "eps"}]}]]], ")"}], "2"], " ", "<>", " ", "There"}], 
    " ", "will", " ", "be", " ", "Eulergamma", " ", "and", " ", "Log", 
    RowBox[{
     RowBox[{"(", "Pi", ")"}], "'"}], "s"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"PLT$AutoConvertToKnownConstants", "=", "False"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"coeffForm", "=", "\"\<\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"analyticCoeffSTR", "=", "\"\<\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fullMIform", "=", "\"\<\\tid mi1L1 = (\\n\\t \>\""}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"loop", " ", "over", " ", "eps"}], "-", "expansion"}], " ", 
   "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"Do", "[", "\n", "\t", 
   RowBox[{
    RowBox[{
     RowBox[{"mi", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"SeriesCoefficient", "[", 
         RowBox[{
          RowBox[{"topo1", "[", "2", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"eps", ",", "0", ",", "epsOrder"}], "}"}]}], "]"}], "/.", 
        RowBox[{
         RowBox[{"PolyGamma", "[", "x__", "]"}], ":>", " ", 
         RowBox[{"FunctionExpand", "[", 
          RowBox[{"PolyGamma", "[", "x", "]"}], "]"}]}]}], "/.", " ", 
       RowBox[{
        RowBox[{"Log", "[", 
         RowBox[{"mUV", "^", "2"}], "]"}], "\[RuleDelayed]", "lMUV"}]}]}], 
     ";", "\n", "\t", 
     RowBox[{"pow", "=", 
      RowBox[{"Exponent", "[", 
       RowBox[{"mi", ",", "lMUV", ",", "Max"}], "]"}]}], ";", "\n", "\t", 
     RowBox[{"(*", " ", 
      RowBox[{"loop", " ", "over", " ", "log", 
       RowBox[{"(", 
        RowBox[{"mUV", "^", "2"}], ")"}], " ", "coefficients", " ", "at", " ",
        "order", " ", 
       RowBox[{"eps", "^", "epsOrder"}]}], " ", "*)"}], "\n", "\t", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", "\t\t", 
      RowBox[{"(*", " ", 
       RowBox[{
       "add", " ", "term", " ", "to", " ", "definition", " ", "of", " ", 
        "eps", " ", "expansion"}], " ", "*)"}], "\n", "\t\t", 
      RowBox[{
       RowBox[{
        RowBox[{"formStr", "=", "\"\<\>\""}], ";", "\[IndentingNewLine]", 
        "\t   ", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"epsOrder", ">=", "0"}], ",", "\[IndentingNewLine]", "\t\t", 
          RowBox[{"fullMIform", "=", 
           RowBox[{"fullMIform", "<>", "\"\<cMi1L1Eps\>\"", "<>", 
            RowBox[{"ToString", "@", "epsOrder"}], "<>", "\"\<lMUV\>\"", "<>", 
            RowBox[{"ToString", "@", "powC"}], "<>", "\"\<*rat(lMUV^\>\"", "<>", 
            RowBox[{"ToString", "@", "powC"}], "<>", "\"\<,1)*rat(eps^\>\"", "<>", 
            RowBox[{"ToString", "@", "epsOrder"}], "<>", 
            "\"\<,1) \\n\\t +\>\""}]}], ",", "\[IndentingNewLine]", "\t\t", 
          RowBox[{"fullMIform", "=", 
           RowBox[{"fullMIform", "<>", "\"\<cMi1L1EpsM\>\"", "<>", 
            RowBox[{"ToString", "@", 
             RowBox[{"(", 
              RowBox[{"-", "epsOrder"}], ")"}]}], "<>", "\"\<lMUV\>\"", "<>", 
            
            RowBox[{"ToString", "@", "powC"}], "<>", "\"\<*rat(lMUV^\>\"", "<>", 
            RowBox[{"ToString", "@", "powC"}], "<>", "\"\<,1)*rat(1,eps^\>\"",
             "<>", 
            RowBox[{"ToString", "@", 
             RowBox[{"(", 
              RowBox[{"-", "epsOrder"}], ")"}]}], "<>", 
            "\"\<) \\n\\t +\>\""}]}]}], "\[IndentingNewLine]", "\t", "]"}], 
        ";", "\n", "\t\t", 
        RowBox[{"(*", " ", 
         RowBox[{"compute", " ", "analytic", " ", "coeff"}], " ", "*)"}], 
        "\[IndentingNewLine]", "\t\t", 
        RowBox[{"miPow", "=", 
         RowBox[{"SeriesCoefficient", "[", 
          RowBox[{"mi", ",", 
           RowBox[{"{", 
            RowBox[{"lMUV", ",", "0", ",", "powC"}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\t\t", 
        RowBox[{"miSimpl", "=", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"miPow", " ", "//", "GToHPL"}], "//", "HPLToG"}], "//",
               "ShuffleG"}], "//", "ExtractZeroes"}], " ", "//", 
            "ComplexExpand"}], "//", "GCoefficientSimplify"}], "//", 
          "Simplify"}]}], ";", "\[IndentingNewLine]", "\t\t   ", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"epsOrder", ">=", "0"}], ",", "\[IndentingNewLine]", 
          "\t\t\t", 
          RowBox[{"anaStr", "=", 
           RowBox[{"StringPartition", "[", 
            RowBox[{
             RowBox[{"\"\<cMi1L1Eps\>\"", "<>", 
              RowBox[{"ToString", "@", "epsOrder"}], "<>", "\"\<lMUV\>\"", "<>", 
              RowBox[{"ToString", "@", "powC"}], "<>", "\"\<=\>\"", "<>", 
              RowBox[{"ToString", "[", 
               RowBox[{"miSimpl", ",", "InputForm"}], "]"}]}], ",", 
             RowBox[{"UpTo", "[", "200", "]"}]}], "]"}]}], ",", 
          "\[IndentingNewLine]", "\t\t\t", 
          RowBox[{"anaStr", "=", 
           RowBox[{"StringPartition", "[", 
            RowBox[{
             RowBox[{"\"\<cMi1L1EpsM\>\"", "<>", 
